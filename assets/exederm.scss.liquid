/*============================================================================
  #Mobile Nav
  - List of items inside the mobile drawer
==============================================================================*/
.mobile-nav {
  // Negative of .drawer left/right padding for full-width link tap area
  margin: (-$gutter / 2) (-$gutter / 2) 0 (-$gutter / 2);

  li {
    margin-bottom: 0;
  }
}

.icon-minus:hover, .icon-plus:hover {
	color: #000;
	background: #3E9A76;
}

.mobile-nav__search {
  padding: $gutter / 2;

  .search-bar {
    margin-bottom: 0;
  }
}

.mobile-nav__item {
  position: relative;
  display: block;

  // Background color on top level items so there is no
  // element overlap on subnav's CSS toggle animation
  .mobile-nav > & {
    background-color: /*$color-primary*/#ffffff;
  }

  &:after {
    content: '';
    position: absolute;
    bottom: 0;
    left: $gutter / 2;
    right: $gutter / 2;
    border-bottom: 1px solid #e9e9e9;
  }

  .mobile-nav > &:last-child:after {
    display: none;
  }
}

// Login/logout links can't have a class on them, so style <a> element
.mobile-nav__item a {
  display: block;
  color:#535353;
}

.mobile-nav__toggle button{
  color:#000;
}

.mobile-nav__item a {
  color:#535353;
}

.mobile-nav__item a:hover {
  color:#ffffff;
  background-color:#3E9A76;
}

.mobile-nav__item a,
.mobile-nav__toggle button {
  padding: 11px 15px;
  text-decoration: none;
  

  &:hover,
  &:active,
  &:focus {
    color: #f7f6ea;
    background-color: #3E9A76;    
  }


  &:active,
  &:focus {
    background-color: #3E9A76;
  }
}

.mobile-nav__toggle button {
	background:transparent;
}

.mobile-nav__item--active {
  font-weight: bold;
}

.mobile-nav__has-sublist {
  display: table;
  width: 100%;

  .mobile-nav__link {
    display: table-cell;
    vertical-align: middle;
    width: 100%;
    border:0 none;
  }
}

.mobile-nav__toggle {
  display: table-cell;
  vertical-align: middle;
  width: 1%;
}

.mobile-nav__toggle-open {
  .mobile-nav--expanded & {
    display: none;
  }
}

.mobile-nav__toggle-close {
  display: none;

  .mobile-nav--expanded & {
    display: block;
  }
}

.mobile-nav li:nth-of-type(3)/*:first-of-type*/{
  .mobile-nav__sublist{
    &:after{
      content: ' ';
      display: table;
      clear: both;
    }
    li:nth-of-type(1),/*:first-of-type,*/ li:nth-of-type(6)/*first-of-type, li:nth-of-type(7)*/{
      border-right: 0;
    
      
      a{
        font-weight: bold;
        padding: 6px 30px 6px 15px;
      }
    }
  }
}


.mobile-nav__sublist {
  margin: 0;
  max-height: 0;
  visibility: hidden;
  overflow: hidden;
  transition: all 300ms cubic-bezier(0.57, 0.06, 0.05, 0.95);
  @include backface();

  .mobile-nav__item{
    background-color:#F4FAFC/*$color-primary-darker*/;
  }

  .mobile-nav--expanded + & {
    visibility: visible;
    max-height: 700px;
    transition: all 700ms cubic-bezier(0.57, 0.06, 0.05, 0.95);
  }

  .mobile-nav__item:after {
    top: 0;
    bottom: auto;
    border-bottom: 0px solid #e3e3e3/*$color-primary-darker-darker*/;
  }

  .mobile-nav__link {
    padding-left: $gutter;
    font-weight: normal;
    color: #535353;
  }
}

/*============================================================================
  #Drawers
==============================================================================*/
.js-drawer-open {
  overflow: hidden;
  height: 100%;
}

.drawer {
  @include promote-layer();
  display: none;
  position: fixed;
  overflow-y: auto;
  overflow-x: hidden;
  -webkit-overflow-scrolling: touch;
  top: 0;
  bottom: 0;
  padding: 0 ($gutter / 2) ($gutter / 2);
  max-width: 95%;
  z-index: $zindexDrawer;
  color: white;
  background-color: $color-primary;
  transition: $drawerTransition;

  a {
    color: #000;
    font-size:14px/*16px*/;
    font-weight:400;
    text-transform: capitalize;

    &:hover,
    &:focus {
      opacity: 1;
    }
  }

  input,
  textarea {
    border-color: $colorDrawerBorder;
    background-color: white;
  }
}

.drawer--left {
  width: $drawerNavWidth;
  left: -$drawerNavWidth;
  border-right: 1px solid $colorDrawerBorder;

  .js-drawer-open-left & {
    display: block;
    @include transform(translateX($drawerNavWidth));

    .lt-ie9 & {
      left: 0;
    }
  }
}

.drawer--right {
  width: $drawerCartWidth;
  right: -$drawerCartWidth;
  border-left: 1px solid $colorDrawerBorder;

  .js-drawer-open-right & {
    display: block;
    @include transform(translateX(-$drawerCartWidth));

    .lt-ie9 & {
      right: 0;
    }
  }
}

#PageContainer {
  overflow: hidden;
}

.is-moved-by-drawer {
  @include promote-layer();
  transition: $drawerTransition;

  .js-drawer-open-left & {
    @include transform(translateX($drawerNavWidth));
  }

  .js-drawer-open-right & {
    @include transform(translateX(-$drawerCartWidth));
  }
}

